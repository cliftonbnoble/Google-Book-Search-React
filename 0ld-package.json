{
  "name": "google-book-search",
  "version": "1.0.0",
  "description": "Google Book Search built with React",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cliftonbnoble/google-book-search.git"
  },
  "keywords": [
    "React",
    "Google",
    "Books"
  ],
  "author": "Clifton Noble",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cliftonbnoble/google-book-search/issues"
  },
  "homepage": "https://github.com/cliftonbnoble/google-book-search#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "mongoose": "^5.5.7",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  }
}
